{
  "name": "tanstack-demo",
  "private": true,
  "type": "module",
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "pnpm format"
    ]
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "tanstack serve",
    "test": "pnpm test:vi:run && pnpm test:pw:run",
    "test:vi": "vitest --watch",
    "test:vi:run": "vitest run",
    "test:vi:coverage": "vitest run --coverage.enabled",
    "test:vi:report": "http-server coverage -p 9087 -s -o ",
    "test:pw:run": "playwright test",
    "test:pw:ui": "playwright test --ui",
    "test:pw:headed": "playwright test --headed",
    "test:pw:debug": "playwright test --debug",
    "test:pw:report": "playwright show-report",
    "test:pw:codegen": "playwright codegen",
    "lint": "eslint",
    "format": "prettier --write **/*",
    "pre-commit": "lint-staged",
    "check": "prettier --write . && eslint --fix",
    "db:setup": "tf custom db:setup",
    "db:push": "tf custom db:push",
    "db:seed": "tf custom db:seed",
    "db:generate": "tf custom db:generate",
    "db:migrate": "tf custom db:migrate",
    "db:pull": "tf custom db:pull",
    "db:studio": "tf custom db:studio",
    "sb:dev": "storybook dev -p 6006",
    "sb:build": "storybook build",
    "prepare": "husky"
  },
  "dependencies": {
    "@t3-oss/env-core": "^0.13.8",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-devtools": "^0.7.0",
    "@tanstack/react-query": "^5.66.5",
    "@tanstack/react-query-devtools": "^5.84.2",
    "@tanstack/react-router": "^1.132.0",
    "@tanstack/react-router-devtools": "^1.132.0",
    "@tanstack/react-router-ssr-query": "^1.131.7",
    "@tanstack/react-start": "^1.132.0",
    "@tanstack/router-plugin": "^1.132.0",
    "@unpic/react": "^1.0.2",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.30.0",
    "drizzle-orm": "^0.39.0",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.544.0",
    "mysql2": "^3.6.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hot-toast": "^2.6.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.6",
    "tanstack-cli": "^0.0.1",
    "taskforge-cli": "^0.0.2",
    "tw-animate-css": "^1.3.6",
    "uid": "^2.0.2",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.2.0",
    "@playwright/test": "^1.57.0",
    "@storybook/addon-a11y": "^10.2.0",
    "@storybook/addon-docs": "^10.2.0",
    "@storybook/react-vite": "^10.2.0",
    "@tanstack/devtools-vite": "^0.3.11",
    "@tanstack/eslint-config": "^0.3.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.2.0",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.10.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "3.2.4",
    "babel-plugin-react-compiler": "^1.0.0",
    "dotenv": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "storybook": "^10.2.0",
    "tsx": "^4.0.0",
    "typescript": "^5.7.2",
    "vite": "^7.1.7",
    "vitest": "^3.0.5",
    "web-vitals": "^5.1.0"
  }
}
